{
    "configurations": [
        {
            "name": "Win64",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../core/src"
            ],
            "defines": [
                "_DEBUG",
                "CORE_BUILD_PLUGIN",
                "CSSCRIPTS_USE_SURVIVAL"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-msvc-x64"
        },
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../core/src",
                "${workspaceFolder}/../LuaJIT/src",
                "${workspaceFolder}/../**"
            ],
            "defines": [
                "_DEBUG",
                "CORE_BUILD_PLUGIN",
                "CSSCRIPTS_USE_SURVIVAL"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-msvc-x86"
        },
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../**",
                "${workspaceFolder}/../core/src/**",
                "/usr/local/include/luajit-2.1"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "defines": [
                "_DEBUG",
                "CORE_BUILD_PLUGIN",
                "CSSCRIPTS_USE_SURVIVAL"
            ],
            "compilerArgs": [],
            "mergeConfigurations": false,
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/../core/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}
